CREATE DATABASE `PublishingHouse` 

-- PublishingHouse.articles definition

CREATE TABLE `articles` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `article_announcement_picture` varchar(100) NOT NULL,
  `title` varchar(100) NOT NULL,
  `id_magazine` bigint NOT NULL,
  `text` varchar(10000) NOT NULL,
  `author` varchar(100) NOT NULL,
  `year_release` bigint NOT NULL,
  PRIMARY KEY (`id`),
  KEY `articles_FK` (`id_magazine`),
  CONSTRAINT `articles_FK` FOREIGN KEY (`id_magazine`) REFERENCES `magazines` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;

-- PublishingHouse.articles definition

CREATE TABLE `articles` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `article_announcement_picture` varchar(100) NOT NULL,
  `title` varchar(100) NOT NULL,
  `id_magazine` bigint NOT NULL,
  `text` varchar(10000) NOT NULL,
  `author` varchar(100) NOT NULL,
  `year_release` bigint NOT NULL,
  PRIMARY KEY (`id`),
  KEY `articles_FK` (`id_magazine`),
  CONSTRAINT `articles_FK` FOREIGN KEY (`id_magazine`) REFERENCES `magazines` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=20 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
